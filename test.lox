class Appliance {
    plug() {
        print "Plugged...";
    }
}

class CoffeeMaker < Appliance {
    init(coffee) {
        this.coffee = coffee;
    }

    plug() {
        print "Plugging the coffee maker.";
        super.plug();
    }

    brew() {
        print "Enjoy your cup of " + this.coffee;
        this.coffee = nil;
    }
}

var maker = CoffeeMaker("colombia");
maker.plug();
maker.brew();